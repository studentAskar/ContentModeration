@page
@model Web.Views.TextContent.ApproveModel

<h2>Approve Video</h2>

<form method="post">
    <div>
        <label>Video ID</label>
        <input asp-for="Command.VideoId" />
    </div>
    <button type="submit">Approve</button>
</form>

@if (Model.ResultMessage != null)
{
    <p>@Model.ResultMessage</p>
}
